{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/auth", "dest": "/api/auth.js" },
      { "src": "/expenses", "dest": "/api/expenses.js" },
      { "src": "/password", "dest": "/api/resetPassword.js" },
      { "src": "/premium", "dest": "/api/premium.js" }
    ]
  }
  